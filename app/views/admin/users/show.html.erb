<h1>
    用户：<%= @user.username %>
</h1>
<p>
    <hr/>
    <% @user.comments_by_commentable(Entry).each do |obj| %>
    <%= obj.commentable.short %><br/><br/>
    <% obj.comments.each do |comment| %>
    <b><%= comment.created_at.to_s %>:</b><%= comment.body %><br/>
    <% end %>
    <hr/>
    <% end %>
</p>

<%= link_to 'Edit', edit_admin_user_path(@user) %> |
<%= link_to 'Back', admin_users_path %>