<% if @entries.count > 0 %>
	<% @entries.each do |entry| %>
		<%= render "entry", :entry => entry %>
	<% end %>
	<div id="logs_info">
		<div id="filter_info">过滤条件:<%#= @filter %></div>
		<div id="total_info">共有<%#= @entries.total_entries %>条日志</div>
		<%#= will_paginate @entries, :renderer => (remote? ? RemotePaginateLinkRenderer : PaginateLinkRenderer) %>
		<div class="clear"></div>
	</div>
<% else %>
	<div class="block" id="not_found">
		未找到符合条件的结果
	</div>
<% end %>